name: MyDns
desc: config for Dns

dns:
  enable: true # set true to enable dns (default is false)
  listen: 127.0.0.1:53
  nameserver:
     - 119.29.29.29
     - 114.114.114.114
  fallback: # concurrent request with nameserver, fallback used when GEOIP country isn't CN
     - https://1.1.1.1/dns-query
     - tls://1.0.0.1:853
     - tls://dns.google:853
